package recapDemo1;

public class main {

	public static void main(String[] args) {
		int sayi1 = 20;
		int sayi2 = 25;
		int sayi3 = 2;
		int enBuyuk = sayi3;
		
		if(enBuyuk <sayi3) {
			enBuyuk = sayi3;
		}
		
		if(enBuyuk <sayi1) {
			enBuyuk = sayi1;
		
		System.out.println("En Buyuk Sayi");

	}

}
}
